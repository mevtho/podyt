{
    "name": "laravel/laravel",
    "type": "project",
    "description": "podyt - build a podcast feed from youtube videos",
    "require": {
        "php": "^8.1",
        "ext-dom": "*",
        "alaouy/youtube": "^v2.2.5",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^v1.3.0",
        "laravel-workflow/laravel-workflow": "^1.0",
        "laravel/framework": "^11.x-dev",
        "laravel/sanctum": "^4.x-dev",
        "laravel/tinker": "^2.7",
        "podyt/podcastfeed": "*",
        "socialiteproviders/youtube": "^4.1",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "jasonmccreary/laravel-test-assertions": "^v2.4.1",
        "laravel-shift/blueprint": "^2.1",
        "laravel/breeze": "^2.x-dev",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^v8.1.1",
        "phpunit/phpunit": "^11.3-dev",
        "spatie/laravel-ignition": "^2.8.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./local-packages/podcastfeed"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
